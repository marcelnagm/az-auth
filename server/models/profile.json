{
  "name": "Profile",
  "plural": "profiles",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true,
    "mysql": {
      "table": "profiles"
    }
  },
  "mixins": {
    "TimeStamp": {
      "createdAt": "createdAt",
      "updatedAt": "updatedAt",
      "required": false,
      "validateUpsert": true,
      "silenceWarnings": false
    },
    "Computed": {
      "properties": {
        "fullName": "getFullName"
      }
    }
  },
  "properties": {
    "firstName": {
      "type": "string",
      "required": true
    },
    "lastName": {
      "type": "string"
    },
    "userId": {
      "type": "number"
    }
  },
  "validations": [],
  "relations": {
    "user": {
      "type": "belongsTo",
      "model": "SysUser"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "SUPERUSER",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
